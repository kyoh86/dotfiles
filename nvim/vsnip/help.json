{
    "help-doc": {
        "prefix": "helpdoc",
        "body": "$1.txt\t(description of the plugin)\n\nAuthor: kyoh86 <me@kyoh86.dev>\nLicense: MIT License\n\n\n==============================================================================\nCONTENTS ~                             *$1-contents*\n\nIntroduction     |$1-introdution|\nFunction         |$1-function|\n\n\n==============================================================================\nFUNCTION                               *$1-function*\n\n\n==============================================================================\nOPTIONS ~                              *$1-function*\n\n\n==============================================================================\n\"vim:tw=78:ts=8:sw=8:ft=help:norl:noet:fen:\n"
    },
    "help-hr": {
        "prefix": "helphr",
        "body": "=============================================================================="
    },
    "help-header": {
        "prefix": "helpheader",
        "body": "$1 ~"
    },
    "help-tag": {
        "prefix": "helptag",
        "body": " *$1* "
    },
    "help-link": {
        "prefix": "helplink",
        "body": " |$1| "
    }
}